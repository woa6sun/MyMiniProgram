/**app.wxss**/
.container{
  display: flex;
  flex-direction: column;
  height: auto;
}

::-webkit-scrollbar
{
width: 6px;
height: 6px;
background-color: #FFF;
}
/*定义滚动条轨道 内阴影+圆角*/
:-webkit-scrollbar-track
{
border-radius: 20px;
background-color: #FFF;
}
/* 定义滑块 */
::-webkit-scrollbar-thumb
{
border-radius: 20px;
background-color: #C3C3C3;
}

